{
  "name": "docs",
  "version": "0.0.1",
  "description": "Awayjs documentation",
  "main": "index.js",
  "scripts": {
    "rimraf": "rimraf",
    "docs": "npm run docs:clear && npm run docs:wrapper && npm run docs:core && npm run docs:graphics && npm run docs:materials && npm run docs:parsers && npm run docs:player && npm run docs:renderer && npm run docs:scene && npm run docs:stage && npm run docs:view",
    "docs:clear": "npm run rimraf -- bin",
    "docs:core":     "node awaydoc.js --name=core --tsconfig=node_modules/@awayjs/core/tsconfig.json --sources=node_modules/@awayjs/core/lib --out=bin/modules/core",
    "docs:graphics": "node awaydoc.js --name=graphics --tsconfig=node_modules/@awayjs/graphics/tsconfig.json --sources=node_modules/@awayjs/graphics/lib --out=bin/modules/graphics",
    "docs:materials": "node awaydoc.js --name=materials --tsconfig=node_modules/@awayjs/materials/tsconfig.json --sources=node_modules/@awayjs/materials/lib --out=bin/modules/materials",
    "docs:parsers": "node awaydoc.js --name=parsers --tsconfig=node_modules/@awayjs/parsers/tsconfig.json --sources=node_modules/@awayjs/parsers/lib --out=bin/modules/parsers",
    "docs:player": "node awaydoc.js --name=player --tsconfig=node_modules/@awayjs/player/tsconfig.json --sources=node_modules/@awayjs/player/lib --out=bin/modules/player",
    "docs:renderer": "node awaydoc.js --name=renderer --tsconfig=node_modules/@awayjs/renderer/tsconfig.json --sources=node_modules/@awayjs/renderer/lib --out=bin/modules/renderer",
    "docs:scene": "node awaydoc.js --name=scene --tsconfig=node_modules/@awayjs/scene/tsconfig.json --sources=node_modules/@awayjs/scene/lib --out=bin/modules/scene",
    "docs:stage": "node awaydoc.js --name=stage --tsconfig=node_modules/@awayjs/stage/tsconfig.json --sources=node_modules/@awayjs/stage/lib --out=bin/modules/stage",
    "docs:view": "node awaydoc.js --name=view --tsconfig=node_modules/@awayjs/view/tsconfig.json --sources=node_modules/@awayjs/view/lib --out=bin/modules/view",
    "docs:wrapper": "node awaydoc.js --name=@awayjs --tsconfig=tsconfig.json --sources=lib --out=bin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awayjs/docs.git"
  },
  "keywords": [
    "awayjs",
    "docs",
    "documentation",
    "typedoc"
  ],
  "author": "thepalebluedot",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/awayjs/docs/issues"
  },
  "homepage": "https://github.com/awayjs/docs#readme",
  "dependencies": {
    "@awayjs/core": "^0.6.62",
    "@awayjs/graphics": "^0.1.11",
    "@awayjs/materials": "^0.3.33",
    "@awayjs/parsers": "^0.5.60",
    "@awayjs/player": "^0.5.29",
    "@awayjs/renderer": "^0.8.58",
    "@awayjs/scene": "^0.8.91",
    "@awayjs/stage": "^0.8.49",
    "@awayjs/view": "^0.2.10",
    "minimist": "^1.2.0",
    "rimraf": "^2.6.1",
    "typedoc": "^0.5.7",
    "typescript": "^2.2.1"
  }
}
